source '/Users/pablosalvador/google-cloud-sdk/completion.zsh.inc'

gcloud iam service-accounts create mlflowserver

gcloud projects add-iam-policy-binding omega-tenure-350518 --member="serviceAccount:mlflowserver@omega-tenure-350518.iam.gserviceaccount.com" --role=roles/viewer

gcloud iam service-accounts keys create credentials.json --iam-account=mlflowserver@omega-tenure-350518.iam.gserviceaccount.com

export GOOGLE_APPLICATION_CREDENTIALS="/Users/pablosalvador/Desktop/GCP/Docu/credentials.json"